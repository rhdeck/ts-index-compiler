{
  "name": "ts-index-compiler",
  "version": "1.1.0",
  "description": "",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "tsc && chmod +x lib/bin.js",
    "docs": "typedoc --theme markdown --readme none --mode file --excludeNotExported --includeVersion --out docstemp && commanderdoc markdown lib/bin.js -o docstemp/__cli.md --cli-name \"npx `npx jq -r .name package.json`\" && rm README.md ; mv docstemp/README.md docstemp/_library.md && concat-md docstemp > README.md && rm -rf docstemp",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhdeck/ts-index-compiler.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhdeck/ts-index-compiler/issues"
  },
  "homepage": "https://github.com/rhdeck/ts-index-compiler#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/node": "^14.0.27",
    "@types/prettier": "^2.0.2",
    "commanderdoc": "^0.1.11",
    "concat-md": "^0.3.5",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.4.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "commander": "^6.0.0",
    "install-peers-cli": "^2.2.0",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "ts-node": "^9.1.1"
  }
}
